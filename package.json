{
  "name": "canya-canpay",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build --prod canpay-lib",
    "npm_pack": "cd dist/canpay-lib && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish": "npm publish dist/canpay-lib/canyaio-canpay-lib-$VERSION.tgz --access public",
    "postinstall": "node patch-web3-crypto.js",
    "init-contracts": "cd ./src && zos init CanPayExample && zos add Dao CanYaCoin",
    "deploy-contracts": "cd ./src && zos push --network local",
    "init-deploy-contracts": "npm run init-contracts && npm run deploy-contracts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/elements": "^6.0.5",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "ebm": "github:easyboxmodel/ebm",
    "hoek": "^4.2.1",
    "ng2-bootstrap-modal": "^1.0.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.2.1",
    "web3": "^1.0.0-beta.34",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "gulp-style-aliases": "^1.1.11",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.3",
    "openzeppelin-solidity": "^1.10.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "zos-lib": "^1.2.1"
  }
}
